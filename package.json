{
    "name": "infuse",
    "version": "0.0.2",
    "private": true,
    "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
    "main": "src/server/index.js",
    "scripts": {
        "setup": "npm install --no-optional --no-shrinkwrap --no-package-lock",
        "start": "node src/server/index.js",
        "build": "NODE_ENV=production && webpack --mode production",
        "client": "npx webpack serve --allowed-hosts all --mode development --devtool inline-source-map --hot",
        "server": "nodemon src/server/index.js",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "test": "jest"
    },
    "author": "Sandeep Raveesh",
    "repository": {
        "type": "git",
        "url": "git://github.com/Patrick-W-McMahon/infuse.git"
    },
    "license": "ISC",
    "dependencies": {
        "assert": "^2.0.0",
        "body-parser": "^1.19.0",
        "express": "^4.16.4",
        "react": "^16.8.6",
        "react-bootstrap": "^1.0.0-beta.8",
        "react-dom": "^16.8.6",
        "react-markdown": "^7.0.1",
        "react-redux": "^7.0.3",
        "react-router": "^5.0.0",
        "react-router-dom": "^5.0.0",
        "react-toastify": "^4.5.2",
        "redux": "^4.0.1",
        "redux-form": "^8.2.0",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "@babel/preset-react": "^7.0.0",
        "@webpack-cli/serve": "^1.5.2",
        "acorn-import-assertions": "^1.7.6",
        "babel-jest": "^24.8.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "chai": "^4.2.0",
        "clean-webpack-plugin": "^4.0.0-alpha.0",
        "concurrently": "^3.5.1",
        "copy-webpack-plugin": "^9.0.1",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.1",
