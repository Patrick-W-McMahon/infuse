    "redux-devtools-extension": "^2.13.8",
    "sinon": "^7.3.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  }
